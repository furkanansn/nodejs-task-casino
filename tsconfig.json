      "experimentalDecorators": true,
      "forceConsistentCasingInFileNames": true,
      "module": "commonjs",
      "esModuleInterop": true,
      "outDir": "./build",
      "rootDir": "./src",
      "target": "es6",
      "skipLibCheck": true,      
      "strict": true,
      "strictPropertyInitialization": false,
  }
}